#include <string>
using namespace std;

string rps(const string& p1, const string& p2)
{
 
  
  if (p1 == "rock" && p2 == "scissors" ){
    return "Player 1 won!";
  }
  else if(p1 == "rock" && p2 == "paper"){
    return "Player 2 won!";
  }
  else if (p1 == "sissors" && p2 == "rock"){
    return "Player 2 won!";
  }
  else if(p1 == "sissors"&& p2 == " paper"){
    return " Player 1 won!";
  }
  else if( p1 == "paper" && p2 == "rock"){
    return "Player 1 won!";
  }
  
  else if(p1 == "paper" && p2 == "sissors"){
    return "player 2 won!";
  }
  
    return "Draw!";
}
